&GLOBAL
   PRINT_LEVEL LOW
   WALLTIME 2682000
   PROJECT H2O
   RUN_TYPE DRIVER
   SEED 1
&END GLOBAL

&MOTION

  &DRIVER
   HOST localhost
   PORT 22342
  &END DRIVER
&END MOTION

&FORCE_EVAL
  METHOD Quickstep

  &DFT
    WFN_RESTART_FILE_NAME H2O-RESTART.wfn
    
    BASIS_SET_FILE_NAME ../../basis/GTH_BASIS_SETS
    POTENTIAL_FILE_NAME ../../basis/GTH_POTENTIALS
    
    &MGRID
      CUTOFF 800
      REL_CUTOFF  60
    &END MGRID
    
    &QS      
      MAP_CONSISTENT
    &END QS
    
    &SCF
        SCF_GUESS RESTART
      EPS_SCF 5.e-7
      MAX_SCF 20
      &OT
   	MINIMIZER DIIS
        N_DIIS 7
        PRECONDITIONER FULL_KINETIC
      &END OT

       &OUTER_SCF
   	 TYPE NONE
	    OPTIMIZER NONE
	    EPS_SCF 5.0E-7
       MAX_SCF 25

       &END OUTER_SCF 
    &END SCF
    
    &XC
     &XC_FUNCTIONAL BLYP
     &END XC_FUNCTIONAL
     &VDW_POTENTIAL
         POTENTIAL_TYPE PAIR_POTENTIAL
         &PAIR_POTENTIAL
            TYPE DFTD3
            R_CUTOFF 16
            REFERENCE_FUNCTIONAL BLYP
            PARAMETER_FILE_NAME ../../basis/dftd3.dat
         &END
     &END
     &XC_GRID
       XC_DERIV SPLINE2
     &END XC_GRID
    &END XC
  &END DFT
  STRESS_TENSOR DIAGONAL_ANALYTICAL

  &SUBSYS
    &CELL
      ABC 10.5 10.5 10.5
      &CELL_REF
        ABC  10.5 10.5 10.5
      &END CELL_REF
    &END CELL

    &KIND H
      BASIS_SET TZV2P-GTH
      POTENTIAL GTH-BLYP-q1
      ELEMENT H
    &END KIND

    &KIND O
      BASIS_SET TZV2P-GTH
      POTENTIAL GTH-BLYP-q6
    &END KIND

